.CostBlock
	padding 9px 12px 22px 19px
	border-radius 12px
	filter drop-shadow(0 0 8px rgba(0,0,0,0.08))
	background-color $white
	
	@media $smPlus
		display grid
		grid-template-columns 48px 1fr auto
		grid-template-rows auto 1fr
		gap 4px 17px
		padding 13px
		
		&::before
			content ""
			grid-area 1 / 1 / 3 / 2
			width 48px
			height 48px
	
	@media $md
		padding 34px 31px 27px
	
	&_sum
		margin-bottom -6px
		color #F89333
		font-weight bold
		font-size 48px
		
		@media $smPlus
			grid-area 1 / 3 / 3 / 4
			margin-top -10px
		
		&-blue
			color $blue
	
	&_title
		font-size 15px
		line-height 22px
		color #28394c
		font-weight 600
		
		@media $smPlus
			grid-area 1 / 2 / 2 / 3
			font-size 18px
	
	&_label
		font-size 13px
		color #9da5b3
		font-weight 400
		
		@media $smPlus
			grid-column 2 / 3
	
