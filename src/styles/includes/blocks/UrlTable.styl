.UrlTable
	border 1px solid #ebf0f2
	font-size 15px
	line-height 1.67
	
	&_item
		display flex
	
	&_url, &_destination
		padding 10px 15px
	
	&_url
		position relative
		flex 180px 0 0
		font-weight 600
		
		&:after
			content ""
			position absolute
			top 4px
			right 0
			bottom 4px
			width 1px
			background #EBF0F2
	
	&_destination
		flex auto 1 1